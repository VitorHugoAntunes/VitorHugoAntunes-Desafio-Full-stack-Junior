{
  "name": "api-gateway",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "build": "nest build --tsc --config tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
   "test": "cross-env NODE_ENV=test vitest run",
    "test:watch": "cross-env NODE_ENV=test vitest",
    "test:cov": "cross-env NODE_ENV=test vitest run --coverage",
    "test:debug": "cross-env NODE_ENV=test vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "cross-env NODE_ENV=test vitest run --config ./vitest.config.e2e.ts",
    "test:e2e:watch": "cross-env NODE_ENV=test vitest --config ./vitest.config.e2e.ts",
    "db:clear": "cross-env NODE_ENV=test ts-node src/scripts/clear-database.ts",
    "db:seed": "cross-env NODE_ENV=test ts-node src/scripts/seed.ts",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d typeorm.config.ts",
    "migration:create": "typeorm-ts-node-commonjs migration:create",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d typeorm.config.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d typeorm.config.ts",
    "migration:show": "typeorm-ts-node-commonjs migration:show -d typeorm.config.ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/websockets": "^11.0.1",
    "@nestjs/throttler": "^2.0.1",
    "@nestjs/swagger": "^8.0.5",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/microservices": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.0.1",
    "@nestjs/typeorm": "^11.0.0",
    "@prisma/client": "^6.17.1",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.3",
    "bcryptjs": "^3.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.27",
    "zod": "^4.1.12",
    "zod-validation-error": "^4.0.2",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.7",
    "@types/socket.io": "^3.0.2",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "globals": "^16.0.0",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.2",
    "cross-env": "^7.0.3",
    "tsconfig-paths": "^4.2.0",
    "vitest": "^3.2.4"
  }
}